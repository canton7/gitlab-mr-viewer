{
    "useTabs": false,
    "tabWidth": 4,
    "trailingComma": "es5",
    "printWidth": 120,
    "bracketSameLine": true,
    "plugins": ["prettier-plugin-svelte"],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        },
        {
            "files": "*.yaml",
            "options": {
                "tabWidth": 2
            }
        }
    ]
}
